{
    "name": "my-network-sipher",
    "version": "1.0.0",
    "channels": {
        "test": {
            "orderers": [
                "orderer.example.com"
            ],
            "peers": {
                "peer0.libertas.sipher.co": {
                    "endorsingPeer": true,
                    "chaincodeQuery": true,
                    "ledgerQuery": true,
                    "eventSource": true
                },
                "peer1.libertas.sipher.co": {
                    "endorsingPeer": true,
                    "chaincodeQuery": true,
                    "ledgerQuery": true,
                    "eventSource": true
                },
                "peer0.libertas.whiteboxplatform.com": {
                    "endorsingPeer": true,
                    "chaincodeQuery": true,
                    "ledgerQuery": true,
                    "eventSource": true
                },
                "peer1.libertas.whiteboxplatform.com": {
                    "endorsingPeer": true,
                    "chaincodeQuery": true,
                    "ledgerQuery": true,
                    "eventSource": true
                }
            }
        }
    },
    "client": {
        "organization": "Sipher",
        "connection": {
            "timeout": {
                "peer": {
                    "endorser": "300"
                }
            }
        }
    },
    "organizations": {
        "Sipher": {
            "mspid": "SipherMSP",
            "peers": [
                "peer0.libertas.sipher.co",
                "peer1.libertas.sipher.co"
            ],
            "certificateAuthorities": [
                "ca.libertas.sipher.co"
            ]
        }
    },
    "peers": {
        "peer0.libertas.sipher.co": {
            "url": "grpcs://127.0.0.1:7051",
            "tlsCACerts": {
                "path": "crypto-config/peerOrganizations/libertas.sipher.co/tlsca/tlsca.libertas.sipher.co-cert.pem"
            },
            "grpcOptions": {
                "ssl-target-name-override": "peer0.libertas.sipher.co"
            }
        },
        "peer1.libertas.sipher.co": {
            "url": "grpcs://127.0.0.1:8051",
            "tlsCACerts": {
                "path": "crypto-config/peerOrganizations/libertas.sipher.co/tlsca/tlsca.libertas.sipher.co-cert.pem"
            },
            "grpcOptions": {
                "ssl-target-name-override": "peer1.libertas.sipher.co"
            }
        }
    },
    "certificateAuthorities": {
        "ca.libertas.sipher.co": {
            "url": "https://127.0.0.1:7054/",
            "caName": "ca-sipher",
            "tlsCACerts": {
                "path": "crypto-config/peerOrganizations/libertas.sipher.co/tlsca/tlsca.libertas.sipher.co-cert.pem"
            },
            "httpOptions": {
                "verify": false
            }
        }
    }
}